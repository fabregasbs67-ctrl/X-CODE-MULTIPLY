{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "index_chat.html", "use": "@vercel/static" },
    { "src": "index_chat_dev.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/dev", "dest": "index_chat_dev.html" },
    { "src": "/chat", "dest": "index_chat.html" },
    { "src": "/", "dest": "index.html" }
  ]
}
